{
  "name": "auto-release-tag",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "release": "node scripts/release.js",
    "release:uat:fix": "node scripts/release.js --env uat --type patch",
    "release:uat:feature": "node scripts/release.js --env uat --type minor",
    "release:stage": "node scripts/release.js --env stage",
    "release:prod": "node scripts/release.js --env prod"
  },
  "devDependencies": {
    "chalk": "4",
    "shelljs": "^0.8.5",
    "yargs": "^17.5.1"
  }
}
